<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.example.computer.chopwell">

    <dist:module dist:instant="true" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter></activity>
        <activity
            android:name=".DetailActivity"
            android:label="@string/title_activity_detail"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".SoupsActivity"
            android:label="Soups and Stews"
            android:parentActivityName=".CategoryActivity" />
        <activity
            android:name=".SnacksActivity"
            android:label="Snacks"
            android:parentActivityName=".CategoryActivity" />
        <activity
            android:name=".SideDishesActivity"
            android:label="Side Dishes"
            android:parentActivityName=".CategoryActivity" />
        <activity
            android:name=".PuddingsActivity"
            android:label="Puddings"
            android:parentActivityName=".CategoryActivity" />
        <activity
            android:name=".MeatActivity"
            android:label="Meats"
            android:parentActivityName=".CategoryActivity" />
        <activity
            android:name=".EntreesActivity"
            android:label="Entrees"
            android:parentActivityName=".CategoryActivity" />
        <activity
            android:name=".BreakfastActivity"
            android:label="Breakfast"
            android:parentActivityName=".CategoryActivity" />
        <activity
            android:name=".BeveragesActivity"
            android:label="Beverages"
            android:parentActivityName=".CategoryActivity" />
        <activity android:name=".MainActivity" android:label="MainActivity">
        </activity>
        <activity
            android:name=".CategoryActivity"
            android:label="Category"
            android:theme="@style/AppTheme" />
    </application>

</manifest>